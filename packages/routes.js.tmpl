const routes = [
  {{range $i,$e:=.}}
  {
      path: '{{$e.Path}}',
      replace: true,
      component: () => import('../{{.Page}}'),
    },
  {{end}}
];

// Always leave this as last one
if (process.env.MODE !== 'ssr') {
  routes.push({
    path: '*',
    replace: true,
    component: () => import('pages/Error404.vue'),
  });
}

export default routes;
